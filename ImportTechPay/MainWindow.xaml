<Window x:Class="ImportTechPay.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ImportTechPay"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dgrResults" IsReadOnly="True" HorizontalAlignment="Left" Height="399" Margin="10,10,0,0" VerticalAlignment="Top" Width="605" AlternationCount="1" RowBackground="AliceBlue" AlternatingRowBackground="LightBlue"/>
        <Button x:Name="btnImportExcel" Content="Import Excel" FontSize="20" HorizontalAlignment="Left" Margin="632,166,0,0" VerticalAlignment="Top" Width="150" Height="50" Click="BtnImportExcel_Click"/>
        <Button x:Name="btnProcess" Content="Process" FontSize="20" HorizontalAlignment="Left" Margin="632,221,0,0" VerticalAlignment="Top" Width="150" Height="50" Click="BtnProcess_Click"/>

    </Grid>
</Window>
